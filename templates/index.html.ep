<style>
  
  .currentPath {
    stroke-width: 2px;
  }

  .x.axis {
    padding-left: 20px;
    padding-right: 20px;
  }

  .page-header {
    padding-top: 0px;
  }

  .axis path,
  .axis line {
    fill: none;
    stroke: grey;
    stroke-width: 1;
    shape-rendering: crispEdges;
  }

 .tick line{
    opacity: 0.2;
  }
  
  #canvas { 
     font: 12px Arial;
  }


  pre {
    font-family: "Courier New"
                    Courier
                    monospace;
    margin-top: 20px;
	text-align: center;	
 }

  .textbox { 
    background: white; 
    border: 1px double #DDD; 
    border-radius: 5px; 
    box-shadow: 0 0 5px #333; 
    color: #666; 
    outline: none; 
	font: 15px;
  }
  
.dygraph-legend {

        padding: 4px;
        border: 1px solid #000;
        border-radius: 10px;
        box-shadow: 4px 4px 4px #888;
        pointer-events: none;
      }

th, td {
    padding: 15px;
}
.label {
  fill: #C4C4C4;
  font: 500 20px "Helvetica Neue";
}

.popover{
    max-width: 60%;
}
 
 a.help {color: "blue"; text-decoration: underline;}
 a.help:visited {color: "blue"; text-decoration: underline;}
 a.help:hover {color: #FFCC00}
 a.help:active {color: #FF00CC}
 
 #accordion-resizer {
   padding: 10px;
   width: 350px;
   height: 300px;
 }
 /* Form Validation */
 span.error {
 font: bold 11px Arial, sans-serif;
 color: red;
}

table#example {
    border-collapse: collapse;  text-align: left; width: 100%; 
}
#example tr {
    border-top: 1px solid #fff;
	font-family: arial, sans-serif;
	font-size: 13px;
}
#example tr:hover {
    background-color: #E1EEF4;
}

#example td {
    padding: 3px 5px; border-bottom: 1px solid #E1EEF4;font-weight: normal;
}
#example td:hover {
    cursor: pointer;
}
#content1,#content2,#screenshots_content1,#screenshots_content2, { display: none; width: 100%; }
</style>

